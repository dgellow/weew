{
  "name": "@weew/weew",
  "version": "0.1.1",
  "exports": "./mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "tasks": {
    "demo": "deno run -A examples/demo.ts",
    "music": "deno run -A examples/music.ts",
    "simple": "deno run -A examples/simple.ts",
    "check": "deno check mod.ts examples/*.ts",
    "test": "deno test -A src/*_test.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
