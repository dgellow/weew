{
  "name": "@weew/weew",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "demo": "deno run -A examples/demo.ts",
    "simple": "deno run -A examples/simple.ts",
    "check": "deno check mod.ts examples/*.ts",
    "test": "deno test -A src/*_test.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
